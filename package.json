{
  "name": "iotf-react",
  "version": "1.0.0",
  "description": "Development environment for IoTF React components",
  "author": "IBM",
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.11",
    "catw": "^1.0.1",
    "gitbook-cli": "^1.0.1",
    "gitbook-plugin-prism": "^1.0.0",
    "gulp": "^3.9.0",
    "gulp-browserify": "^0.5.1",
    "gulp-less": "^3.0.5",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.7",
    "lodash.assign": "^4.0.3",
    "rimraf": "^2.5.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react",
            "stage-1"
          ]
        }
      ],
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "./examples/public/vendor/react/react-dom.js": "ReactDOM",
    "./examples/public/vendor/react/react-with-addons.js": "React",
    "./examples/public/vendor/jquery/dist/jquery.min.js": "jquery",
    "./examples/public/vendor/jquery-autoellipsis/jquery.autoellipsis-1.0.10.min.js": "jquery-autoellipsis",
    "./examples/public/vendor/jquery-ui/jquery-ui.min.js": "jquery-ui",
    "./examples/public/vendor/reflux/reflux.min.js": "reflux",
    "./examples/public/vendor/react-grid-layout/react-grid-layout.js": "react-grid-layout",
    "./examples/public/vendor/react-grid-layout/utils.js": "react-grid-layout-utils",
    "./examples/public/vendor/mqttws31/index.js": "mqttws31",
    "./examples/public/vendor/d3/d3.min.js": "d3",
    "./examples/public/vendor/dygraphs/dygraph-combined.js": "dygraphs",
    "./examples/public/vendor/c3/c3.min.js": "c3",
    "./examples/public/vendor/openlayers/ol.js": "openlayers",
    "./examples/public/vendor/griddle-react/griddle.js": "griddle-react",
    "./examples/public/vendor/griddle-react/underscore-min.js": "_",
    "./examples/public/vendor/object-assign/index.js": "object-assign",
    "./examples/public/vendor/eventemitter3/index.js": "eventemitter3",
    "./examples/public/vendor/moment/moment.js": "moment",
    "react": "global:React",
    "ReactDOM": "global:ReactDOM",
    "react-dom": "global:ReactDOM",
    "React": "global:React",
    "moment": "global:moment",
    "mqttws31": "global:Messaging",
    "jquery": "global:jQuery",
    "jquery-autoellipsis": "global:jQueryAutoellipsis",
    "jquery-ui": "global:jQueryUI",
    "reflux": "global:Reflux",
    "react-grid-layout": "global:ReactGridLayout",
    "react-grid-layout/build/utils": "global:ReactGridLayoutUtils",
    "d3": "global:d3",
    "dygraphs": "global:Dygraph",
    "c3": "global:c3",
    "openlayers": "global:openlayers",
    "griddle-react": "global:Griddle",
    "_": "global:_",
    "object-assign": "global:object-assign",
    "eventemitter3": "global:eventemitter3"
  },
  "scripts": {
    "watch": "gulp",
    "start": "node examples/app.js",
    "docs:prepare": "gitbook install",
    "docs:build": "npm run docs:prepare && gitbook build -g IoT/iotf-react",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:clean": "rimraf _book",
    "docs:publish": "npm run docs:clean && npm run docs:build && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.ibm.com:IoT/iotf-react.git gh-pages --force"
  }
}
