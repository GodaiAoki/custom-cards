{
  "name": "iotf-react",
  "version": "1.0.0",
  "description": "Development environment for IoTF React components",
  "author": "IBM",
  "dependencies": {
    "body-parser": "^1.15.0",
    "cfenv": "1.0.x",
    "express": "4.12.x",
    "https": "^1.0.0",
    "request": "^2.67.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.11",
    "catw": "^1.0.1",
    "gitbook-cli": "^1.0.1",
    "gitbook-plugin-prism": "^1.0.0",
    "gulp": "^3.9.0",
    "gulp-browserify": "^0.5.1",
    "gulp-less": "^3.0.5",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.7",
    "lodash.assign": "^4.0.3",
    "rimraf": "^2.5.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react",
            "stage-1"
          ]
        }
      ],
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "./public/vendor/react/react-dom.js": "ReactDOM",
    "./public/vendor/react/react-with-addons.js": "React",
    "./public/vendor/jquery/dist/jquery.min.js": "jquery",
    "./public/vendor/jquery-autoellipsis/jquery.autoellipsis-1.0.10.min.js": "jquery-autoellipsis",
    "./public/vendor/jquery-ui/jquery-ui.min.js": "jquery-ui",
    "./public/vendor/reflux/reflux.min.js": "reflux",
    "./public/vendor/react-grid-layout/react-grid-layout.js": "react-grid-layout",
    "./public/vendor/react-grid-layout/utils.js": "react-grid-layout-utils",
    "./public/vendor/mqttws31/index.js": "mqttws31",
    "./public/vendor/d3/d3.min.js": "d3",
    "./public/vendor/dygraphs/dygraph-combined.js": "dygraphs",
    "./public/vendor/c3/c3.min.js": "c3",
    "./public/vendor/openlayers/ol.js": "openlayers",
    "./public/vendor/griddle-react/griddle.js": "griddle-react",
    "./public/vendor/griddle-react/underscore-min.js": "_",
    "./public/vendor/object-assign/index.js": "object-assign",
    "./public/vendor/eventemitter3/index.js": "eventemitter3",
    "./public/vendor/moment/moment.js": "moment",
    "./public/vendor/iotp/IoTCommon.js": "IoTFCommon",
    "./public/vendor/iotp/IoTComponents.js": "IoTComponents",
    "react": "global:React",
    "ReactDOM": "global:ReactDOM",
    "react-dom": "global:ReactDOM",
    "React": "global:React",
    "moment": "global:moment",
    "mqttws31": "global:Messaging",
    "jquery": "global:jQuery",
    "jquery-autoellipsis": "global:jQueryAutoellipsis",
    "jquery-ui": "global:jQueryUI",
    "reflux": "global:Reflux",
    "react-grid-layout": "global:ReactGridLayout",
    "react-grid-layout/build/utils": "global:ReactGridLayoutUtils",
    "d3": "global:d3",
    "dygraphs": "global:Dygraph",
    "c3": "global:c3",
    "openlayers": "global:openlayers",
    "griddle-react": "global:Griddle",
    "_": "global:_",
    "object-assign": "global:object-assign",
    "eventemitter3": "global:eventemitter3",
    "IoTComponents": "global:IoTComponents",
    "IoTFCommon": "global:IoTFCommon"
  },
  "scripts": {
    "watch": "gulp",
    "start": "node app.js"
  }
}
